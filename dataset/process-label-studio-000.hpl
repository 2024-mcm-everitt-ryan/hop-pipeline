        <replace>Y</replace>
        <scriptResult>N</scriptResult>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <name>text_neutral</name>
        <precision>-1</precision>
        <rename>text_neutral</rename>
        <replace>Y</replace>
        <scriptResult>N</scriptResult>
