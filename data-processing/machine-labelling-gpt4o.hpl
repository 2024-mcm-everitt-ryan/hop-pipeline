   label_masculine = str[2] == '1'
   label_feminine = str[3] == '1'
   label_racial = str[4] == '1'
   label_sexuality = str[5] == '1'
   label_general = str[6] == '1'

  except Exception as e:
    print(e)
    rejected = True</scriptBody>
