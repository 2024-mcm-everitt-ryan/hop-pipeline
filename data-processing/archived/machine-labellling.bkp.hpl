    <scriptLanguage>python</scriptLanguage>
    <scripts>
      <script>
        <scriptBody>category="age"

votes = [
    phi3,
    llama3,
    claude,
]
weights = [
    0.2,
    0.5,
    0.8,
]


# Helper function to check if the vote is valid (0, 1 or None)
def is_valid(vote):
    return vote in [0, 1, None]

# Convert invalid votes to None
votes = [vote if is_valid(vote) else None for vote in votes]

# Set default weights if none provided
if weights is None:
    weights = [1] * len(votes)

# Filter out None votes for calculations
valid_votes = [vote for vote in votes if vote is not None]
valid_weights = [weights[i] for i in range(len(votes)) if votes[i] is not None]

# Majority Vote
if len(valid_votes) == 0:
    majority_vote = False
else:
    majority_vote = sum(valid_votes) > len(valid_votes) / 2

# Average Vote
if len(valid_votes) == 0:
    average_vote = False
else:
    average_vote = sum(valid_votes) / len(valid_votes) > 0.5

# Weighted Vote
if len(valid_votes) == 0:
    weighted_vote = False
else:
    weighted_average = sum(v * w for v, w in zip(valid_votes, valid_weights)) / sum(valid_weights)
    weighted_vote = weighted_average > 0.5
